        "@pliancy/eslint-config-ts": "1.0.0",
        "@pliancy/semantic-release-config-npm": "2.2.0",
        "@types/jest": "29.5.4",
        "commitizen": "4.3.0",
        "cpy-cli": "5.0.0",
        "husky": "8.0.3",
        "jest": "29.6.4",
        "npm-run-all": "4.1.5",
        "pinst": "3.0.0",
        "rimraf": "5.0.1",
        "semantic-release": "23.0.2",
        "ts-jest": "29.1.1",
        "ts-node": "10.9.1",
