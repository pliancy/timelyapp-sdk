{
    "name": "timelyapp",
    "version": "2.1.2",
    "description": "SDK for timelyapp.com API",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rimraf dist coverage tmp",
        "lint": "eslint \"./**/*.{ts,js,json,yml}\" --fix",
        "test": "jest",
        "test:ci": "jest --coverage",
        "test:watch": "jest --watch",
        "build": "yarn clean && yarn lint && tsc",
        "build:ci": "tsc",
        "build:check": "tsc --noEmit",
        "build:watch": "yarn clean && tsc -w",
        "postinstall": "[ -d '.husky' ] && husky install || true",
        "prepublishOnly": "yarn build"
    },
    "keywords": [],
    "author": {
        "name": "Josh Stout",
        "email": "joshstout@gmail.com"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/pliancy/timelyapp-sdk"
    },
    "devDependencies": {
        "@pliancy/eslint-config-ts": "^0.0.5",
        "@types/jest": "27.0.1",
        "dotenv": "^8.2.0",
        "husky": "^6.0.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "np": "^7.3.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.2"
    },
    "engines": {
        "node": ">=12"
    },
    "volta": {
        "node": "14.16.1",
        "yarn": "1.22.10"
    },
    "dependencies": {
        "axios": "^0.21.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && yarn build:check"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --cache --fix"
        ]
    },
    "np": {
        "yarn": false,
        "branch": "main"
    }
}
